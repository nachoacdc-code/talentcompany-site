---
const to = '/services/marketing/';
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta http-equiv="refresh" content={`0; url=${to}`} />
		<meta name="robots" content="noindex" />
		<link rel="canonical" href={to} />
		<title>Redirecting…</title>
	</head>
	<body>
		<p>
			Redirecting to <a href={to}>{to}</a>…
		</p>
		<script is:inline>{`window.location.replace(${JSON.stringify(to)});`}</script>
	</body>
</html>

